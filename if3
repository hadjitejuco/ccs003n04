//Day of the Week (1 - 7) 
//1 -> Monday
//2 -> Tuesday
//..
//7 -> Sunday
#include <iostream>
using namespace std;
int main(){
	int day;
	cout << "Enter no: ";
	cin >> day;
	if (day==1){
		cout << "Monday";
	}else if (day==2){
		cout << "Tuesday";
	}else if (day==3){
		cout << "WED";
	}else if (day==4){
		cout << "THURS";
	}else if (day==5){
		cout << "FRI";
	}else if (day==6){
		cout << "SAT";
	}else if (day==7){
		cout << "SUN";
	}else{
		cout << "Invalid ";
	}
	return 0;
}



