//constants and variables
//constants -> fix values
#include <iostream>
using namespace std;
int main(){
	//declare variables (local)
	int a,b,c;
	//assign a value to variables
	//syntax (var_name = value;)
	a = 10;		//assign 10 to a
	b = 20;		//assign 20 to b
	c = a+b;	//operation (operator +)
	cout << "sum is " << c;
	return 0;
}
