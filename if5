/*
	Range			Grade
	90 - 100		A
	80-89			B
	70-79			C
	60-69			D
	below 60		F		
*/
#include <iostream>
using namespace std;
int main(){
	int g;
	cout << "\nEnter grade: ";
	cin >> g;
	//LOGICAL OPERATOR
	//AND OR NOT
	if (g>=90 && g<=100)
		cout << "\nGrade is A";
	else if (g>=80 && g<=89)
		cout << "\nGrade is B";
	else if (g>=70 && g<=79)
		cout << "\nGrade is C";
	else if (g>=60 && g<=69)
		cout << "\nGrade is D";
	else if (g>=50 && g<=59)
		cout << "\nGrae is F";
	else
		cout << "\nInvalid input";
	return 0;
}





